{
    "name": "dev-extensions/thumbnail-generator",
    "description": "Media thumbnails generator plugin",
    "type": "library",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dev\\ThumbnailGenerator\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dev\\ThumbnailGenerator\\Providers\\ThumbnailGeneratorServiceProvider"
            ],
            "aliases": {
                "ThumbnailMedia": "Dev\\ThumbnailGenerator\\Facades\\ThumbnailMediaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
